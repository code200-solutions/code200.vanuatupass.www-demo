{
  "expo": {
    "name": "vanuatuPass-www-demo",
    "slug": "vanuatupass-demo",
    "scheme": "demo",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "8ef96a72-e35c-49c9-ac0a-eada89bc33b1"
      }
    },
    "owner": "code200_solutions",
    "icon": "./src/assets/images/logo.png",
    // ANDROID CONFIGURATION
    "android": {
      "adaptiveIcon": {
        "monochromeImage": "./src/assets/logos/android/icon_512.png",
        "backgroundColor": "#6A3DE8"
      },
      // icon for older Android devices that do not support Adaptive Icons
      "icon": "./src/assets/logos/android/icon_512.png",
      "package": "solutions.code200.vanuatupass.demo"
    },
    // IOS CONFIGURATION
    "ios": {
      "bundleIdentifier": "solutions.code200.vanuatupass.demo",
      "icon": "./src/assets/logos/ios/AppIcon~ios-marketing.png",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": ["35F9.1"] // 'app uses access to local storage to save some configuration data made by the user. This information is not shared with any third party nor sent off-device.'
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": ["E174.1"] // 'app uses access to local storage to save some configuration data made by the user. This information is not shared with any third party nor sent off-device.'
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": ["C617.1"] // 'app uses access to local storage to save some configuration data made by the user. This information is not shared with any third party nor sent off-device.'
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"] // 'app uses access to local storage to save some configuration data made by the user. This information is not shared with any third party nor sent off-device.'
          }
        ]
      }
    },
    // WEB CONFIGURATION
    "web": {
      "output": "static"
    }
  }
}
